    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "src/**.js"
    ],
    "exclude": [
      "src/public/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [],
    "extension": [],
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  }
